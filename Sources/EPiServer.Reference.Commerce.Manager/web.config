      <add verb="*" path="*.image.ashx" type="Mediachase.Commerce.Catalog.Handlers.CatalogImageHandler,Mediachase.Commerce" />
      <remove path="*.asmx" verb="*" />
      <remove path="*.xsd" verb="*" />
