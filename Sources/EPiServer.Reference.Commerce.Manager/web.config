  <FrameworkProviders>
    <dataService defaultProvider="SqlDataProvider">
      <providers>
        <add name="SqlDataProvider" type="Mediachase.Data.Provider.SqlDataProvider, Mediachase.SqlDataProvider" connectionStringName="EcfSqlConnection" applicationName="CommerceSample" />
      </providers>
    </dataService>
    <imageService defaultProvider="HandlerImageProvider">
      <providers>
        <add name="HandlerImageProvider" type="Mediachase.Commerce.Engine.Images.Providers.HandlerImageProvider, Mediachase.Commerce" urlFormatString="~/images/t/{0}.image.ashx" applicationName="CommerceSample" />
      </providers>
    </imageService>
    <urlService>
      <providers>
      </providers>
    </urlService>
    <siteService>
      <providers>
      </providers>
    </siteService>
  </FrameworkProviders>
  <Mediachase.RssNews>
    <Items>
      <add urlPath="http://world.episerver.com/Articles?feed=RSS" culture="en-US" newsCount="7" />
    </Items>
  </Mediachase.RssNews>
