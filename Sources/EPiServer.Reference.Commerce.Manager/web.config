    <add key="DictionaryFolder" value="~/_cmsbase/Dictionaries" />
    <add key="CuteEditorLicense" value="~/_cmsbase/Design/licenses/CuteEditor.lic" />
    <add key="MCCatalogDefaultImage" value="~/Templates/Everything/styles/images/nopic.png" />
    <!-- Admin Settings -->
    <add key="ShellFirstPageUrl" value="~/Apps/Shell/Pages/ContentFrame.aspx" />
    <add key="AppsDir" value="~/Apps" />
    <add key="AdminHtmlEditorControl" value="~/Apps/Core/Controls/Editors/TinyMCE/EditorControl.ascx" />
    <add key="CommerceManagerHelpUrlPrefix" value="http://docs.mediachase.com/display/ECFG52CommerceGuide/" />
    <!-- Import Export Folder -->
    <add key="ECF.ImportExportFolder" value="~/app_data/ImportExport/" />
    <!-- Global Default Settings -->
    <add key="DefaultCountryCode" value="GBR" />
    <add key="SearchApiUrl" value="http://forkquicksilver/" />
    <!--Feedback URL-->
    <add key="FeedbackUrl" value="http://www.mediachase.com/catalog/Contact-Us,135.aspx" />
    <add key="UseLegacyAssetSystem" value="false" />
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <!-- File library-->
  <mediachase.ibn.library>
    <folderElementLocations configSource="Configs\baf.library.folders.config" />
    <downloadFilters>
      <add name="thumbnail" type="Mediachase.Commerce.Assets.DownloadFilterThumbnail, Mediachase.Commerce" />
    </downloadFilters>
    <folderElementExtension>
      <add mimeType="image/jpeg" type="Mediachase.Commerce.Assets.ImageFolderElementExtension, Mediachase.Commerce" />
      <add mimeType="application/pdf" type="Mediachase.Commerce.Assets.PdfFolderElementExtension, Mediachase.Commerce" />
    </folderElementExtension>
  </mediachase.ibn.library>
  <!-- Business Foundation Configuration -->
